{
  "name": "@bracify/gui",
  "version": "1.0.0",
  "description": "GUI Launcher for Bracify",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-rebuild -f -w sqlite3",
    "copy-engine": "rm -rf engine && cp -R ../engine engine",
    "package": "npm run package:x64 && npm run package:arm64",
    "package:x64": "npm run copy-engine && electron-packager . bracify-studio --platform=darwin --arch=x64 --overwrite",
    "package:arm64": "npm run copy-engine && electron-packager . bracify-studio --platform=darwin --arch=arm64 --overwrite"
  },
  "author": "Bracify Team",
  "license": "MIT",
  "dependencies": {
    "@bracify/engine": "file:../engine",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsdom": "^20.0.3",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9"
  }
}